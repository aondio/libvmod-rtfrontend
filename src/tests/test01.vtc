varnishtest "Test libvmod-rtfrontend vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import libvmod-rtfrontend from "${vmod_topbuild}/src/.libs/libvmod_libvmod-rtfrontend.so";

	sub vcl_deliver {
		set resp.http.hello = libvmod-rtfrontend.hello("World");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.hello == "Hello, World"
}

client c1 -run
